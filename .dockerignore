# Version control
.git                                # Git repository
.gitignore                         # Git ignore file
.github                            # GitHub specific files

# Dependencies
node_modules                       # Node.js modules
.pnpm-store                       # PNPM store

# Build outputs
build                             # Build directory
.svelte-kit                       # SvelteKit output
/.svelte-kit
/build

# Development files
.vscode                           # VSCode settings
.idea                             # IntelliJ settings
*.log                             # Log files
.DS_Store                         # macOS system files
coverage                          # Test coverage reports

# Environment files
.env                              # Local environment file
.env.*                            # All environment files
!.env.development                 # Except development env
!.env.production                  # Except production env

# Temporary files
*.tmp                             # Temporary files
*.temp                            # Alternative temp files

# Test artifacts
test-screenshots/                 # Test screenshots
playwright-report/                # Playwright reports
auth-failure-*.png               # Auth failure screenshots
debug*.png                       # Debug screenshots
redirect*.png                    # Redirect screenshots
test-failure*.png               # Test failure screenshots
test-results/                    # Test results directory